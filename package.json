    "knex": "^0.17.3",
    "morgan": "^1.9.1",
    "pg": "^7.11.0",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "postgrator-cli": "^3.1.0",
    "supertest": "^4.0.2"
  }
